{
    "version":2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      },
      {
        "src": "/cart",
        "dest": "/"
      },
      {
        "src": "/success",
        "dest": "/"
      }
    ]
  }